grid = [['.', '.', '.', '.', '.', '.'],        ['.', 'O', 'O', '.', '.', '.'],        ['O', 'O', 'O', 'O', '.', '.'],        ['O', 'O', 'O', 'O', 'O', '.'],        ['.', 'O', 'O', 'O', 'O', 'O'],        ['O', 'O', 'O', 'O', 'O', '.'],        ['O', 'O', 'O', 'O', '.', '.'],        ['.', 'O', 'O', '.', '.', '.'],        ['.', '.', '.', '.', '.', '.']]
for x in range (len(grid[:][0])):
  for y in range ((len(grid)-1),-1,-1):
    if y ==0:
        print (grid[y][x])
    else:
        print(grid[y][x], end=' ')
